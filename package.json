{
  "name": "slack-notifier-cf",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.ts",
  "dependencies": {
    "@google-cloud/functions-framework": "1.1.1",
    "@slack/webhook": "5.0.0",
    "@types/chai": "4.1.7",
    "@types/chai-http": "4.2.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.4",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "copyfiles": "2.1.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "typescript": "3.5.1"
  },
  "devDependencies": {},
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "test": "mocha dist/test/**",
    "copy_deps": "copyfiles package.json env.yaml dist/src/",
    "start": "cd dist/src; functions-framework --target=cloudBuildNotifier",
    "_deploy": "cd dist/src; gcloud functions deploy cloudBuildNotifier --runtime nodejs10 --trigger-topic cloud-builds --env-vars-file env.yaml",
    "deploy": "run-s clean build copy_deps _deploy"
  },
  "author": "Takashi Matsuno",
